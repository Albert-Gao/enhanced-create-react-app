{
  "name": "enhanced-create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "formatAllCode": "./node_modules/.bin/prettier --write './src/**/*.js'",
    "inspectBundle": "node ./inspectBundle.js",
    "update": "yarn upgrade-interactive && syncyarnlock --save"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "prettier": "^1.15.1",
    "syncyarnlock": "^1.0.11",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
